  type : uri_folder
outputs:
 train_data: 
  type: uri_folder
 test_data: 
  type: uri_folder
code: ./data_prep.py
environment:
  # for this step, we'll use an AzureML curate environment
  azureml:AzureML-sklearn-0.24-ubuntu18.04-py37-cpu:21
command: >-
  python data_prep.py
  --data ${{}}
  --train_data ${{inputs.train_data}} 
  --test_data ${{inputs.test_data}} 
  
